
<Gencase>
# int

libmesh_gen 1   
mgops_gen   1

</Gencase>


<TimeLoop>
# double

dt          0.1  
itime       0.0  
nsteps      5    
printstep   1    
ndigits     4    
restart     0    # ==== both for the App and for Gencase ======

</TimeLoop>


<Box>
# double # mesh file consistency (case libmesh_gen=0)  

 lxb 0.    
 lxe 1.  

 lyb 0.  
 lye 2.  

 lzb 0.  
 lze 1.  

# only for libmesh_gen=1

nintervx   8
nintervy   8 
nintervz   8

transflag 0       # ==== not in library routines, but library-type
thetax    0.       # ==== not in library routines, but library-type
thetay    0.       # ==== not in library routines, but library-type
thetaz    0.       # ==== not in library routines, but library-type
# thetaz0.523598775598

</Box>



<Utils>
# double # mesh file consistency

# ------<Mesh>---------------------------------------
nolevels      3                 
domain        0           # Box, or whatever SHAPE...  # =MESH CONSISTENCY(libmesh_gen=0)===  # ==== in library routines   # 0=Box# 1=Cylinder# Whatever the rest
meshql        1           # ------<ElementMapping>------- # for THE APP # =MESH CONSISTENCY=== # ==== in library routines # depends on the kind of Mesh that is used# depends basically on the Boundary Mesh# 0=QUADRATIC Mapping  1=LINEAR Mapping
mesh_ord      0           # ==== both for the App and for Gencase ======  # ==== DO IT ONLY FOR Gencase ======  # GAMBIT mesh should provide this info# 0=QUADRATIC GeomElements (Quad8,Quad9,Hex27,...)# 1=LINEAR GeomElements (Quad4,Hex8,...)
numgeomels    1           # quadrilateral,triangular,pyramidal,...        # ==== DO IT ONLY FOR Gencase ======  # GAMBIT mesh should provide this info# no support for hybrid mesh yet
geomel_type   0           # ==== DO IT ONLY FOR Gencase ======  # GAMBIT mesh should provide this info
dimension     2              


# ----<BCHandling>-----------------
ibc_gen     0            # ==== in library routines
bdry_toll   0.0000001    # ==== not in library routines, but library-type
penalty_val 1.e10        # ==== not in library routines, but library-type

</Utils>


<Files>
# std::string  # mesh file consistency

INPUT_DIR     input/
OUTPUT_DIR    output/
F_MESH_READ    inclQ2D2x2.gam
# ======== this last one is Only Gencase # we recognize the file format from the extension: .gam=gambit, .med=salome

</Files>



<Physics>
# double

# ReferenceValueForNon-dimensionalizationVelLenTemperature
Uref      1.
Lref      1.
Tref      1.

# FluidProperties 

# Monophase
rho0      1.
mu0       1.
kappa0    1.
cp0       1.
komp0     0.

# Two-Phase
rho1      1.
mu1       1.
kappa1    1.
cp1       1.
komp1    0.

# SurfaceTension
sigma   1.

# VolumetricHeatSource
qheat 0.

# FSI
rhos 7890.

# ----------------------------------------------------------------------
# ------ for the Gravity "Quantity" (NS, STRUCT-MECH) ---------------------
# -----------------------------------------------------------------------
# Gravity-0.101937-cos60+sin60=-0.0509685+0.0882800315856
dirgx  0.
dirgy  0.
dirgz  0.

# ----------------------------------------------------------------------
# ------ for the MHD Quantities ---------------------
# ----------------------------------------------------------------------- 
MUMHD 1.e-6
SIGMHD 1.e+6
Bref 5.e-3

# ----------------------------------------------------------------------
# ------ for the MHDCONT Equation class ---------------------------------------
# -----------------------------------------------------------------------
# BC FLAGS
# the next one is a flag, I'll put it here for convenience
# 0 means false, so FIX
# 1 means true, so CONTROL
UseControl 1

# ----------------------------------------------------------------------
# ------ for the Box:Domain class ---------------------------------------
# -----------------------------------------------------------------------
# true or fake 3D simulation
Fake3D 1

# ----------------------------------------------------------------------
# ------ for every Equation class ---------------------------------------
# -----------------------------------------------------------------------
# PseudoTimeStepping
NonStatNS 0 # 1
NonStatMHD 0 # 1
NonStatTEMP 0

 alphaT 1000 
alphaL2  1.
alphaH1  1. # LEAVE IT THERE

 injsuc 2.0 


</Physics>