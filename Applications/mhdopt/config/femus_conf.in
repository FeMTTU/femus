NEW_RUN      new_run

# ----  BCHandling  --------------
# external ibc file
IBC           case 
BDRY_SUFFIX   _bd

# --------------------------------------------------
# ----  MyApp log  --------------
# --------------------------------------------------
RUN_LOG    run
PETSC_FEMUS_LOG  petsc_main.log 

# **********************NON_LIBRARY, specific application *****************************
# now, here is a problem: WE ABSOLUTELY MUST DISTINGUISH  BETWEEN LIBRARY STRINGS and APPLICATION STRINGS

# ------- OPT CONTROL -----------
INTEGRAL   integral.txt


</Files>


# ================================================================================================================================
# ============== runtime parameters for the Physics class ========================================================================
# ================================================================================================================================



<Physics>

# ReferenceValueForNon-dimensionalizationVelLenTemperature
Uref      1.
Lref      1.
Tref      1.

# FluidProperties 

# Monophase
rho0      1.
mu0       1.
kappa0    1.
cp0       1.
komp0     0.

# Two-Phase
rho1      1.
mu1       1.
kappa1    1.
cp1       1.
komp1    0.

# SurfaceTension
sigma   1.

# HeatSource
qheat 0.

# FSI
rhos 7890.

# ----------------------------------------------------------------------
# ------ for the Gravity "Quantity" (NS, STRUCT-MECH) ---------------------
# -----------------------------------------------------------------------
# Gravity-0.101937-cos60+sin60=-0.0509685+0.0882800315856
dirgx  0.
dirgy  0.
dirgz  0.

# ----------------------------------------------------------------------
# ------ for the MHD Quantities ---------------------
# ----------------------------------------------------------------------- 
MUMHD 1.e-6
SIGMHD 1.e+6
Bref 5.e-3

# ---------------------------------------------------------------------------
# ------ for Some CONTROL Equations and for the DesVelocity Quantity ----------
# ---------------------------------------------------------------------------
alphaVel 100. 
betaL2    0.
gammaLap  1.
 udes    -1 # comment it0.4

# ----------------------------------------------------------------------
# ------ for the MHDCONT Equation class ---------------------------------------
# -----------------------------------------------------------------------
# BC FLAGS
# the next one is a flag, I'll put it here for convenience
# 0 means false, so FIX
# 1 means true, so CONTROL
UseControl 1

# ----------------------------------------------------------------------
# ------ for the Box:Domain class ---------------------------------------
# -----------------------------------------------------------------------
# true or fake 3D simulation
# 0 = true 3D; 1= symmetry conditions
Fake3D 0

# ----------------------------------------------------------------------
# ------ for every Equation class ---------------------------------------
# -----------------------------------------------------------------------
# PseudoTimeStepping
NonStatNS 0 # 1
NonStatMHD 0 # 1
NonStatNSAD 0 
NonStatMHDAD 0 
NonStatMHDCONT 0

</Physics>

